{
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/build",
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "JWT_SECRET": "zhixing-jwt-secret-key-2025",
    "JWT_EXPIRES_IN": "24h",
    "CORS_ORIGIN": "https://zhixing.vercel.app"
  }
}