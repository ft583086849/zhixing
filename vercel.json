{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "JWT_EXPIRES_IN": "24h",
    "CORS_ORIGIN": "https://zhixing-new.vercel.app"
  },
  "functions": {
    "server/index.js": {
      "maxDuration": 30
    }
  }
} 