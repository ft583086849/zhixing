{
  "name": "payment-management-system",
  "version": "1.0.0",
  "description": "支付管理系统",
  "main": "server/index.js",
  "scripts": {
    "start": "cd server && npm start",
    "build": "cd server && npm install",
    "install-server": "cd server && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "build-client": "cd client && npm run build",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "deploy-sqlite": "chmod +x deploy-sqlite.sh && ./deploy-sqlite.sh",
    "local-start": "chmod +x start.sh && ./start.sh",
    "migrate": "cd server && node scripts/migrate.js",
    "test": "cd test && node test-system.js"
  },
  "keywords": [
    "payment",
    "management",
    "system"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.3",
    "sequelize": "^6.37.7",
    "uuid": "^9.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
